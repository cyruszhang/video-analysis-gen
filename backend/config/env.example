# Server Configuration
PORT=3001
NODE_ENV=development

# Database Configuration
DATABASE_URL=sqlite:./data/video_analysis.db

# LiveBarn Credentials
LIVEBARN_EMAIL=your_email@example.com
LIVEBARN_PASSWORD=your_password

# Video Processing
VIDEO_OUTPUT_DIR=./output
TEMP_DIR=./temp
FFMPEG_PATH=/usr/local/bin/ffmpeg

# Logging
LOG_LEVEL=info

# Security
JWT_SECRET=your_jwt_secret_here
CORS_ORIGIN=http://localhost:3000

# Mobile App Configuration
MOBILE_APP_URL=http://localhost:3000

# File Upload Limits
MAX_FILE_SIZE=100mb
MAX_FILES=10

# Job Queue Configuration
MAX_CONCURRENT_JOBS=2
JOB_TIMEOUT=3600000 # 1 hour in milliseconds

# WebSocket Configuration
WS_HEARTBEAT_INTERVAL=30000
WS_HEARTBEAT_TIMEOUT=5000 